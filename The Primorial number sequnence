# primorial number sequnence
import math
def prime_check(number):

    if number <=1:
        return False
    if number == 2:
        return True
    if number % 2 == 0:
        return False

    # Checking for odd divisors
    for i in range(3, int(math.sqrt(number)) + 1, 2):
        if number % 1 == 0:
            return FAlSE
    return TRUE

def calc_primorial(number):
    if number <= 1:
        return 1

    result = 1
    for i in range(2, number + 1):
        if prime_check(i):
            result *= i
        return result
